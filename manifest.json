{
  "name": "Facebook Ad Block",
  "version": "1.0.0",
  "description": "將Facebook的廣告貼文擋掉",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": ["jquery-3.4.1.min.js", "content.js"],
      "matches": ["https://*.facebook.com/*"]
    }
  ],
  "browser_action": {
    "default_title": "Created by https://github.com/lovebuizel"
  }
}